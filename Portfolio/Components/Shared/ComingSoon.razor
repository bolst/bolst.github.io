@namespace Portfolio.Components

<MudContainer Fixed="true">
    <MudText Class="d-flex justify-start pb-6" Typo="Typo.h6" Color="Color.Primary">
        Currently working on...
    </MudText>
    <MudDivider DividerType="DividerType.Middle" />
        <MudText Class="d-flex justify-center py-2" Typo="Typo.h5" Color="Color.Primary">
            Breaking the Factoring Problem with Quantum Algorithms
        </MudText>
    <MudDivider DividerType="DividerType.Middle" />

    <MudGrid Class="pt-6 pb-4">
        <MudItem Class="d-flex align-center" xs="4">
            <MudLink Href="https://www.python.org/" Target="blank" Underline="Underline.None">
                <MudImage Class="rounded-lg" Src="https://cdn.freebiesupply.com/logos/large/2x/python-5-logo-png-transparent.png"
                    Fluid="true" />
            </MudLink>
        </MudItem>
        <MudItem Class="d-flex align-center" xs="4">
            <MudLink Href="https://www.ibm.com/quantum/qiskit" Target="blank" Underline="Underline.None">
                <MudImage Class="rounded-lg"
                    Src="https://yt3.googleusercontent.com/mBeBCw-OeX3gqZX2r0IwwCctQYSyRUb1bqppQiAQ-6qvlPoTxBO1cBXBw8BIJWYXjHy20jddwA=s900-c-k-c0x00ffffff-no-rj"
                    Fluid="true" />
            </MudLink>
        </MudItem>
        <MudItem Class="d-flex align-center" xs="4">
            <MudLink Href="https://quantumai.google/cirq" Target="blank" Underline="Underline.None">
                <MudImage Class="rounded-lg"
                    Src="https://quantumai.google/static/site-assets/images/marketing/icons/shared-ic-cirq.png"
                    Fluid="true" />
            </MudLink>
        </MudItem>
    </MudGrid>

    <MudContainer Class="d-flex justify-center pt-2 px-6">
        <MudImage Class="rounded-lg"
            Src="https://miro.medium.com/v2/resize:fit:1000/1*cno1u_GOb2lAuTMmG-e14g.gif" Elevation="5"
            Fluid="true" />
    </MudContainer>
</MudContainer>
